<?xml version="1.0"?>
<configuration>
  
  <connectionStrings>
    <add name="AppMetricsConnectionString"
      connectionString="Data Source=VM2012SRV;Initial Catalog=TaxiGuardian;Persist Security Info=True;User ID=TaxiGuardian;Password=taxiguardian;Application Name=WCFAppMetrics;Connect Timeout=20"
      providerName="System.Data.SqlClient"/>
  
  <!--<add name="AppMetricsConnectionString"
      connectionString="Data Source=192.168.8.87;Initial Catalog=AppMetrics;Persist Security Info=True;User ID=AppMetrics;Password=verdeonda;Application Name=WCFAppMetrics;Connect Timeout=20"
      providerName="System.Data.SqlClient"/>-->
        
  </connectionStrings>
  
  <system.web>
    <compilation targetFramework="4.0" />
    <httpRuntime maxRequestLength="524288" />
    <!-- customErrors mode="Off"/ -->
  </system.web>
  
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="metadata">
          <webHttp/>
        </behavior>
      </endpointBehaviors>
      <!--Uncomment next serviceBehaviors tag for publish metadata (for testing)-->
      <serviceBehaviors>
        <behavior name="MyServiceTypeBehaviors" >
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />
    <services>
      <!--Comment next service tag for publish metadata (for testing)-->
      <!--<service name="WCFAppMetrics.Web">
        <endpoint address="" behaviorConfiguration="metadata" binding="webHttpBinding"
                  contract="WCFAppMetrics.IWeb" bindingConfiguration="webBinding"/>
      </service>
      <service name="WCFAppMetrics.Mobile">
        <endpoint address="" behaviorConfiguration="metadata" binding="webHttpBinding"
                  contract="WCFAppMetrics.IMobile" bindingConfiguration="webBinding"/>
      </service>-->
      <!--Uncomment next service tag for publish metadata (for testing)-->
      <service name="WCFAppMetrics.Web" behaviorConfiguration="MyServiceTypeBehaviors" >
        <endpoint contract="WCFAppMetrics.IWeb" binding="mexHttpBinding" address="mex"/>
      </service>
      <service name="WCFAppMetrics.Mobile" behaviorConfiguration="MyServiceTypeBehaviors" >
        <endpoint contract="WCFAppMetrics.IMobile" binding="mexHttpBinding" address="mex"/>
      </service>
    </services>
    <bindings>
      <webHttpBinding>
        <binding name="mexHttpBinding" maxReceivedMessageSize="4194304"></binding>
        <binding name="webBinding">
          <!--Uncomment next security tag for https configuration-->
          <!--<security mode="Transport">
          </security>-->
        </binding>
      </webHttpBinding>
    </bindings>    
  </system.serviceModel>
  
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
  </system.webServer>

  <appSettings>
    <!-- Log Configuration-->
    <add key="LogPath" value=""/>
    <add key="LogSize" value="4096"/>
    <add key="LogBufferSize" value="32"/>
    <add key="CacheAbsoluteExpirationTime" value="60"/>
    <!-- CONF. MAIL -->
    <add key="SMTP_PORT" value="25" />
    <add key="SMTP_HOST" value="mail.taxiguardian.net" />
    <add key="SMTP_USER" value="info@taxiguardian.net" />
    <add key="SMTP_PASSWORD" value="M3v0y3nt4x1" />
    <add key="SMTP_FROM" value="info@taxiguardian.net" />
    <add key="SMTP_TO" value="info@taxiguardian.net" />
    <add key="SMTP_TO_MEXICO" value="contacto-mx@taxiguardian.net" />
    <add key="SMTP_MAIL_CCO" value="" />
  </appSettings>
</configuration>
